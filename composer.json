{
    "name": "open-dosa/laravel-sbadmin-starterkit",    
    "authors": [
        {
            "name": "thomaspaulson",
            "email": "thomas.paulson@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/console": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0",        
        "thomaspaulson/vendor-plugin": "^1.5"
    },
    "autoload": {
		"psr-4": {
			"LastGrade\\SBAdmin\\": "src/"
		}
    },	
    "extra": {
        "expose": [
            "client/dist/css",
            "client/dist/js",
            "client/dist/img",
            "client/dist/vendor"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thomaspaulson/vendor-plugin"
        }
    ]
}